license	first_name	last_name	team_name	member_from	member_to	ccx_only	print_card	card_printed_at	membership_card	date_of_birth	occupation	street	city	state	zip	wants_mail	email	wants_email	home_phone	work_phone	cell_fax	gender	road_category	track_category	ccx_category	mtb_category	dh_category	ccx_number	dh_number	road_number	singlespeed_number	track_number	xc_number	notes	volunteer_interest	official_interest	race_promotion_interest	team_interest	created_at	updated_at
<%- @people.each do |person| -%>
<%= to_excel person["license"] -%>
	<%= to_excel person["first_name"] -%>
	<%= to_excel person["last_name"] -%>
	<%= to_excel person["team_name"] -%>
	<%= Time.zone.utc_to_local(person["member_from"].to_time).to_s(:mdY) if person["member_from"] -%>
	<%= Time.zone.utc_to_local(person["member_to"].to_time).to_s(:mdY) if person["member_to"] -%>
	<%= person["ccx_only"] -%>
	<%= person["print_card"] -%>
	<%= person["card_printed_at"] -%>
	<%= to_excel person["membership_card"] -%>
	<%= Time.zone.utc_to_local(person["date_of_birth"].to_time).to_s(:mdY) if person["date_of_birth"] -%>
	<%= to_excel person["occupation"] -%>
	<%= to_excel person["street"] -%>
	<%= to_excel person["city"] -%>
	<%= to_excel person["state"] -%>
	<%= to_excel person["zip"] -%>
	<%= person["wants_mail"] -%>
	<%= to_excel person["email"] -%>
	<%= person["wants_email"] -%>
	<%= to_excel person["home_phone"] -%>
	<%= to_excel person["work_phone"] -%>
	<%= to_excel person["cell_fax"] -%>
	<%= to_excel person["gender"] -%>
	<%= to_excel person["road_category"] -%>
	<%= to_excel person["track_category"] -%>
	<%= to_excel person["ccx_category"] -%>
	<%= to_excel person["mtb_category"] -%>
	<%= to_excel person["dh_category"] -%>
	<%= to_excel person["ccx_number"] -%>
	<%= to_excel person["dh_number"] -%>
	<%= to_excel person["road_number"] -%>
	<%= to_excel person["singlespeed_number"] -%>
	<%= to_excel person["track_number"] -%>
	<%= to_excel person["xc_number"] -%>
	<%= to_excel person["notes"] -%>
	<%= person["volunteer_interest"] -%>
	<%= person["official_interest"] -%>
	<%= person["race_promotion_interest"] -%>
	<%= person["team_interest"] -%>
	<%= Time.zone.utc_to_local(person["created_at"]).to_s(:mdY) if person["created_at"] -%>
  <%# Last column is slightly different to ensure a line break -%>
	<%= Time.zone.utc_to_local(person["updated_at"]).to_s(:mdY) if person["updated_at"] %>
<%- end -%>

    <% if @auto_complete_fields -%>
    <script type="text/javascript" charset="utf-8">
      <% @auto_complete_fields.each do |auto_complete_field| -%>
        <% if auto_complete_field[:attribute] == :team %>
        <%= "autoCompleteTeam('#{auto_complete_field[:model]}', '#{auto_complete_field[:attribute]}', '#{auto_complete_field[:path]}')".html_safe %>
        <% else %>
        <%= "autoComplete('#{auto_complete_field[:model]}', '#{auto_complete_field[:attribute]}', '#{auto_complete_field[:path]}')".html_safe %>
        <% end -%>
      <% end -%>
    </script>
    <% end -%>
